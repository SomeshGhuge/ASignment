<?xml version="1.0" encoding="UTF-8"?>
 <beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
    
 <bean id = "cat" class="ByConstructor.Categories" autowire="constructor">
       <constructor-arg value="Life Insurance"></constructor-arg>
    </bean>
    <bean id = "policy" class="ByConstructor.Policy">
    <constructor-arg value="Term Plan"></constructor-arg>
    <constructor-arg value="15000"></constructor-arg>
    </bean>
   
</beans>

   
   

    
        
    
  
    
    
  
    